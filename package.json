{
  "name": "fullstack-gcp-demo",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "type": "module",
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "dev": "concurrently -n web,api -c auto \"pnpm dev:web\" \"pnpm dev:server\"",
    "dev:web": "vite --host 0.0.0.0 --port 5173",
    "dev:server": "tsx watch --clear-screen=false server/index.ts",
    "dev:api": "pnpm dev:server",
    "build": "pnpm build:web && pnpm build:server",
    "build:web": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview --port 5173",
    "start": "node dist/server/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "express": "^5.0.0",
    "drizzle-orm": "^0.44.6",
    "pg": "^8.12.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/express": "^5.0.0",
    "@types/node": "^20.12.12",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/pg": "^8.11.7",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.17.0",
    "vite": "^5.4.8",
    "vitest": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "jsdom": "^25.0.0"
  }
}
